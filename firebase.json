{
  "projects": {
    "default": "orca-904de"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "database": {
    "rules": "config/database.rules.json"
  },
  "storage": {
    "rules": "config/storage.rules"
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "database": {
      "port": 9000
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}
